{
  "version": 2,
  "rewrites": [
    {
      "source": "/cashtracking/api/:match*",
      "destination": "/api/cashtracking"
    },
    {
      "source": "/cashtracking/static/:match*",
      "destination": "/CashTracking/frontend/build/static/:match*"
    },
    {
      "source": "/cashtracking/manifest.json",
      "destination": "/CashTracking/frontend/build/manifest.json"
    },
    {
      "source": "/cashtracking/favicon.ico",
      "destination": "/CashTracking/frontend/build/favicon.ico"
    },
    {
      "source": "/cashtracking/logo192.png",
      "destination": "/CashTracking/frontend/build/logo192.png"
    },
    {
      "source": "/gams/api/:match*",
      "destination": "/api/gams"
    },
    {
      "source": "/legal/api/:match*",
      "destination": "/api/legal"
    },
    {
      "source": "/gams/public/:match*",
      "destination": "/gams/public/:match*"
    },
    {
      "source": "/gams",
      "destination": "/gams/public/dashboard.html"
    },
    {
      "source": "/gams/",
      "destination": "/gams/public/dashboard.html"
    },
    {
      "source": "/gams/login",
      "destination": "/gams/public/login.html"
    },
    {
      "source": "/gams/dashboard",
      "destination": "/gams/public/dashboard.html"
    },
    {
      "source": "/gams/requests",
      "destination": "/gams/public/requests.html"
    },
    {
      "source": "/gams/stock",
      "destination": "/gams/public/stock.html"
    },
    {
      "source": "/gams/calendar",
      "destination": "/gams/public/calendar.html"
    },
    {
      "source": "/gams/accounts",
      "destination": "/gams/public/accounts.html"
    },
    {
      "source": "/gams/stock-report",
      "destination": "/gams/public/stock-report.html"
    },
    {
      "source": "/gams/:match*",
      "destination": "/gams/public/:match*"
    },
    {
      "source": "/legal",
      "destination": "/legal/frontend/public/dashboard.html"
    },
    {
      "source": "/legal/",
      "destination": "/legal/frontend/public/dashboard.html"
    },
    {
      "source": "/legal/license-permit",
      "destination": "/legal/frontend/public/license-permit.html"
    },
    {
      "source": "/legal/regulatory",
      "destination": "/legal/frontend/public/regulatory.html"
    },
    {
      "source": "/legal/elibrary",
      "destination": "/legal/frontend/public/elibrary.html"
    },
    {
      "source": "/legal/operational",
      "destination": "/legal/frontend/public/operational.html"
    },
    {
      "source": "/legal/company",
      "destination": "/legal/frontend/public/company.html"
    },
    {
      "source": "/legal/safety",
      "destination": "/legal/frontend/public/safety.html"
    },
    {
      "source": "/legal/:match*",
      "destination": "/legal/frontend/public/:match*"
    },
    {
      "source": "/cashtracking",
      "destination": "/CashTracking/frontend/build/index.html"
    },
    {
      "source": "/cashtracking/",
      "destination": "/CashTracking/frontend/build/index.html"
    },
    {
      "source": "/cashtracking/:match*",
      "destination": "/CashTracking/frontend/build/index.html"
    },
    {
      "source": "/healthcare-web",
      "destination": "/healthcare-web/index.html"
    },
    {
      "source": "/healthcare-web/",
      "destination": "/healthcare-web/index.html"
    }
  ]
}
