      "destination": "/CashTracking/frontend/build/favicon.ico"
    },
    {
      "source": "/cashtracking/logo192.png",
      "destination": "/CashTracking/frontend/build/logo192.png"
    },
    {
      "source": "/cashtracking/:match*",
      "destination": "/CashTracking/frontend/build/index.html"
    }
  ]
}