    },
    {
      "source": "/regulatory.html",
      "destination": "/legal/frontend/public/regulatory.html"
    },
    {
      "source": "/regulatory",
      "destination": "/legal/frontend/public/regulatory.html"
    },
    {
      "source": "/elibrary.html",
      "destination": "/legal/frontend/public/elibrary.html"
    },
    {
      "source": "/elibrary",
      "destination": "/legal/frontend/public/elibrary.html"
    },
    {
      "source": "/healthcare-web",
      "destination": "/healthcare-web/index.html"
    },
    {
      "source": "/healthcare-web/",
      "destination": "/healthcare-web/index.html"
    },
    {
      "source": "/healthcare-web/:match*",
      "destination": "/healthcare-web/:match*"
    },
    {
      "source": "/cashtracking/static/:match*",
      "destination": "/CashTracking/frontend/build/static/:match*"
    },
    {
      "source": "/cashtracking/manifest.json",
